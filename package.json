    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "testcontainers": "^11.5.1"
  }
}
